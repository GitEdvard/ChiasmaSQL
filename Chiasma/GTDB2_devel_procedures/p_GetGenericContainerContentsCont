/****** Object:  StoredProcedure [dbo].[p_GetGenericContainerContentsContainersOnly]    Script Date: 11/20/2009 15:59:07 ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
-- Get containers inside this container.
ALTER PROCEDURE [dbo].[p_GetGenericContainerContentsContainersOnly] (
	@id INTEGER)

AS
BEGIN

DECLARE @select_command VARCHAR (1024)

SET @select_command = 'SELECT container_id as generic_container_id FROM container
				WHERE container_id IN (SELECT child_container_id FROM contents
								WHERE parent_container_id = ' + STR(@id) + ')'

EXECUTE p_GetGenericContainers @select_command

END
